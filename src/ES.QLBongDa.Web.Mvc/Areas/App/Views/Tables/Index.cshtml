@model ES.QLBongDa.Web.Areas.App.Models.Tables.IndexViewModel

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Document</title>
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>
<body>
<div class="container">

    <div class="row kt-margin-b-5">
        <div class="col-xs-6">
            <div class="page-head">
                <div class="page-title">
                    <h1>
                            <span style="font-weight:bold">@L("Tables")</span>
                    </h1>
                </div>
            </div>
        </div>
    </div>

    <div id="AllTableList" class="list-group">
        <table style="border-radius:100px" class="table table-hoover">
                <tr style="background-color:lightgray;font-weight :bold">
                <td>Tên CLB</td>
                <td>Năm</td>
                <td>Vòng</td>
                <td>Trận</td>
                <td>Thắng</td>
                <td>Hòa</td>
                <td>Thua</td>
                <td>Hiệu Số</td>
                <td>Điểm</td>
            </tr>

    @foreach (var personListDto in Model.Items)
        {
            <tr>
                <td>@personListDto.tenclb</td>
                <td>@personListDto.nam</td>
                <td>@personListDto.vong</td>
                <td>@personListDto.vong</td>
                <td>@personListDto.thang</td>
                <td>@personListDto.hoa</td>
                <td>@personListDto.thua</td>
                <td>@personListDto.hieuso</td>
                <td>@personListDto.diem</td>
            </tr>
        }
        </table>
    </div>
</div>
<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
	<script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</body>
</html>